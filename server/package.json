{
  "name": "express-postgres-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Starter project using Express and Postgres",
  "main": "config.js",
  "scripts": {
    "test": "xo",
    "lint": "xo",
    "format": "xo --fix",
    "start": "node ./bin/start.js",
    "dev": "nodemon ./bin/start.js",
    "migrate": "node ./bin/migrate.js",
    "migrate:create": "migrate create --migrations-dir='./src/migrations'"
  },
  "keywords": [
    "express",
    "postgres"
  ],
  "author": "Hugo Di Francesco",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "client-sessions": "^0.8.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openid-connect": "^2.5.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "migrate": "^1.7.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "sql-template-strings": "^2.2.2",
    "uuid": "^8.3.2",
    "vue": "^2.6.14",
    "webpack": "^5.63.0"
  },
  "devDependencies": {
    "jest": "^27.3.1",
    "nodemon": "^2.0.12",
    "xo": "^0.36.1"
  },
  "xo": {
    "prettier": true,
    "space": true
  }
}
